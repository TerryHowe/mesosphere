openstack_security_groups:
  default:
    - port_min: 1
      port_max: 65535
      direction: egress
      protocol: tcp
      remote_ip_prefixes:
        - 0.0.0.0/0
    - port_min: 1
      port_max: 65535
      direction: egress
      protocol: udp
      remote_ip_prefixes:
        - 0.0.0.0/0
    - port_min: 1
      port_max: 65535
      direction: ingress
      protocol: tcp
      remote_ip_prefixes:
        - 192.168.0.0/16
    - port_min: 1
      port_max: 65535
      direction: ingress
      protocol: udp
      remote_ip_prefixes:
        - 192.168.0.0/16
    - port_min: -1
      port_max: -1
      direction: egress
      protocol: icmp
      remote_ip_prefixes:
        - 0.0.0.0/0
    - port_min: -1
      port_max: -1
      direction: ingress
      protocol: icmp
      remote_ip_prefixes:
        - 192.168.0.0/16
    - port_min: 22
      remote_ip_prefixes:
        - 0.0.0.0/0
      state: absent

